{
    "name": "dachcom-digital/i18n",
    "type": "pimcore-bundle",
    "license": "GPL-3.0-or-later",
    "description": "Pimcore I18n Manager.",
    "keywords": ["pimcore", "i18n", "localization", "internationalization"],
    "homepage": "https://github.com/dachcom-digital/pimcore-i18n",
    "authors": [
        {
            "name": "DACHCOM.DIGITAL Stefan Hagspiel",
            "email": "shagspiel@dachcom.ch",
            "homepage": "http://www.dachcom.com/",
            "role": "Developer"
        }
    ],
    "autoload": {
        "psr-4": {
            "I18nBundle\\": "src/I18nBundle"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "": "src/"
        },
        "classmap": [
            "app/AppKernel.php"
        ]
    },
    "extra": {
        "pimcore": {
            "bundles": [
                "I18nBundle\\I18nBundle"
            ]
        }
    },
    "conflict": {
        "pimcore/pimcore": "6.4.*"
    },
    "require": {
        "pimcore/pimcore": "^6.0"
    },
    "require-dev": {
        "codeception/codeception": "~2.4.0",
        "phpstan/phpstan": "^0.12",
        "phpstan/phpstan-symfony": "^0.12",
        "symplify/easy-coding-standard": "^5.4"
    }
}
